<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/blue">

    <ImageView
        android:id="@+id/logo"
        android:layout_width="130dp"
        android:layout_height="130dp"
        android:src="@drawable/logo"
        android:translationX="120dp"
        android:translationY="309dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_skin"
        android:layout_width="142dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/league_spartan_thin"
        android:text="Skin"
        android:textColor="@color/white"
        android:textSize="48sp"
        android:gravity="center"
        android:translationX="114dp"
        android:translationY="458dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_first"
        android:layout_width="142dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/league_spartan_thin"
        android:text="First"
        android:textColor="@color/white"
        android:textSize="48sp"
        android:gravity="center"
        android:translationX="114dp"
        android:translationY="496dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_dermatology"
        android:layout_width="142dp"
        android:layout_height="wrap_content"
        android:fontFamily="@font/league_spartan_semibold"
        android:text="Dermatology Center"
        android:textColor="@color/white"
        android:textSize="12sp"
        android:gravity="center"
        android:translationX="114dp"
        android:translationY="549dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>